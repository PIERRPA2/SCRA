<?xml version="1.0" encoding="UTF-8"?>
<database>
    <element label="Request" max_length="40" name="x_novrp_cr_request" type="collection">
        <element default="PIERRPA2" label="Assigned To" max_length="32" name="assigned_to" reference="sys_user" type="reference"/>
        <element default="javascript:global.getNextObjNumberPadded();" display="true" label="Number" max_length="40" name="number" read_only="true" type="string"/>
        <element choice="3" default="planning" label="Status" max_length="40" name="status" type="string">
            <choice>
                <element label="Planning" sequence="1" value="planning"/>
                <element label="Inspection" sequence="2" value="inspection"/>
                <element label="Rework" sequence="3" value="rework"/>
                <element label="Complete" sequence="4" value="complete"/>
            </choice>
        </element>
        <element label="Update Set" max_length="32" name="update_set" reference="sys_update_set" type="reference"/>
        <index name="index">
            <element name="update_set"/>
        </index>
        <index name="index2">
            <element name="assigned_to"/>
        </index>
    </element>
</database>
